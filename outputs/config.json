{
  "data_dir": "./data",
  "dataset": "cifar10",
  "output_dir": "./outputs",
  "model": "emn_small",
  "variant": "small",
  "width_mult": 1.0,
  "dropout": 0.2,
  "img_size": 224,
  "batch_size": 64,
  "epochs": 100,
  "num_workers": 4,
  "lr": 0.001,
  "min_lr": 1e-06,
  "weight_decay": 0.05,
  "optimizer": "adamw",
  "scheduler": "cosine",
  "warmup_epochs": 5,
  "clip_grad": 1.0,
  "grad_accum_steps": 1,
  "label_smoothing": 0.1,
  "use_focal": false,
  "auto_augment": true,
  "randaugment_n": 2,
  "randaugment_m": 9,
  "use_mixup": true,
  "mixup_alpha": 0.2,
  "cutmix_alpha": 1.0,
  "mixup_prob": 0.5,
  "use_ema": true,
  "ema_decay": 0.9999,
  "amp": true,
  "resume": "",
  "print_freq": 50
}